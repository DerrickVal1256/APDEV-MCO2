<!DOCTYPE html>
<html>
<head>
    <title>Lab Reservation</title>

    <link href="https://fonts.google.com/share?selection.family=Inter:wght@100..900|Kumbh+Sans:wght@400;700" rel="stylesheet">
    <link href="../css/EditRes.css" rel="stylesheet">
    <link href="../css/sidebar.css" rel="stylesheet">

</head>
<body>
    {{> sidebar}}
    <div id="main">

        <h1>Edit Reservations</h1> 
        {{#each reservations}}
        <div class="box">
            <p><b>{{this.lab}}</b></p>
            <p>Seat <b>{{this.seat}}</b></p>
            <p>{{this.time}}</p>
            <p>{{this.date}}</p>
            <a href="ReservationAdmin.html"><button class="edit-btn">Edit</button></a>
        </div>
        {{/each}}

    </div>
</body>
</html>
